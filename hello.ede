import json

let name = "foo";
let age = 10.5;

for i = range [1..10] {
    if (i == 4) {
        age = age + i
    }
    age++;
};
println("name is", name, "and age is", age);

let obj = match (json.parse(`{"subjects":["english", "french"]}`)) {
    case ERROR: <- printf("subjects is not json", ERROR)
}

let best_subject = obj.subjects[1]
println("best subject is ", best_subject)